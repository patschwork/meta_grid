    <data>
      <line>
        <item>SELECT 
   CAST(TABLE_CATALOG AS VARCHAR(1500)) AS "CATALOG"
  ,CAST(TABLE_SCHEMA AS VARCHAR(1500)) AS "SCHEMA"
  ,CAST(TABLE_NAME AS VARCHAR(1500)) AS "NAME"
  ,CAST('VIEW' AS VARCHAR(1500)) AS "TYPE"
  ,CAST(pg_get_viewdef(CONCAT(TABLE_SCHEMA, '.', TABLE_NAME)) AS TEXT) AS "DEFINITION"
  ,CAST(pg_catalog.obj_description(c.oid, 'pg_class') AS VARCHAR(4000)) AS "DESCRIPTION"
  ,CAST(c.oid AS INT) AS "RDBMS_internal_object_id"
  ,CAST(CONCAT('"', TABLE_CATALOG, '"', '.', '"', TABLE_SCHEMA, '"', '.', '"', TABLE_NAME, '"') AS VARCHAR(255)) AS "LOCATION"
  ,CAST(UPPER(CONCAT('"', TABLE_CATALOG, '"', '.', '"', TABLE_SCHEMA, '"', '.', '"', TABLE_NAME, '"')) AS VARCHAR(255)) AS "LOCATION_UPPER"
FROM pg_catalog.pg_class c
LEFT JOIN pg_catalog.pg_namespace n ON
   1 = 1 
   AND n.oid = c.relnamespace
LEFT JOIN pg_catalog.pg_stat_all_tables s ON
   1 = 1 
   AND c.oid = s.relid
LEFT JOIN INFORMATION_SCHEMA.VIEWS isv ON
   1 = 1
   AND isv.TABLE_SCHEMA = n.nspname
   AND isv.TABLE_NAME   = c.relname
WHERE 1 = 1
   AND c.relkind  = 'v'
ORDER BY
  1,2,3</item>
        <item>PGSQL</item>
      </line>
      <line>
        <item>SELECT 
   CAST(TABLE_CATALOG AS VARCHAR(1500)) AS "CATALOG"
  ,CAST(TABLE_SCHEMA AS VARCHAR(1500)) AS "SCHEMA"
  ,CAST(TABLE_NAME AS VARCHAR(1500)) AS "NAME"
  ,CAST('VIEW' AS VARCHAR(1500)) AS "TYPE"
  ,CAST(OBJECT_DEFINITION(obj.object_id) AS NVARCHAR(MAX)) AS "DEFINITION"
  ,CAST(ext.[value] AS VARCHAR(4000)) AS "DESCRIPTION"
  ,CAST(obj.object_id AS INT) AS "RDBMS_internal_object_id"
  ,CAST(CONCAT('"', TABLE_CATALOG, '"', '.', '"', TABLE_SCHEMA, '"', '.', '"', TABLE_NAME, '"') AS VARCHAR(255)) AS "LOCATION"
  ,CAST(UPPER(CONCAT('"', TABLE_CATALOG, '"', '.', '"', TABLE_SCHEMA, '"', '.', '"', TABLE_NAME, '"')) AS VARCHAR(255)) AS "LOCATION_UPPER"
FROM sys.objects obj
LEFT JOIN sys.extended_properties ext ON obj.object_id = ext.major_id AND ext.name = 'MS_Description'
LEFT JOIN INFORMATION_SCHEMA.VIEWS isv ON
    1 = 1
    AND isv.TABLE_SCHEMA = SCHEMA_NAME(obj.schema_id)
    AND isv.TABLE_NAME   = obj.NAME
WHERE 1 = 1
    AND obj.type_desc = 'VIEW'
ORDER BY
  1,2,3</item>
        <item>MSSQL</item>
      </line>
