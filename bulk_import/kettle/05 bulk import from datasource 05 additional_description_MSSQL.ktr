    <sql>SELECT
     [Parent_Catalog]   = DB_NAME()
	,[Parent_Schema]    = SCHEMA_NAME(obj.schema_id)
	,[Parent_Name]      = obj.NAME
	,[Parent_Type]      = CAST(obj.type AS VARCHAR(1))
	,[Parent_Type_Desc] = obj.type_desc
	,[Obj_Type]         = CAST(IIF(col.NAME IS NOT NULL,'COLUMN',IIF(par.NAME IS NOT NULL, 'PARAMETER',NULL)) AS VARCHAR(100))
	,[Obj_Name]         = CAST(IIF(col.NAME IS NOT NULL,col.NAME,IIF(par.NAME IS NOT NULL, par.NAME,NULL)) AS VARCHAR(1500))
    ,[Description]      = CAST(ext.value AS NVARCHAR(MAX))
	,[Ext_prop]         = CAST(ext.name AS VARCHAR(100))
FROM sys.extended_properties ext
LEFT JOIN sys.objects obj ON obj.object_id = ext.major_id
LEFT JOIN sys.columns col ON col.object_id = ext.major_id
	AND col.column_id = ext.minor_id
LEFT JOIN sys.parameters par ON par.object_id = ext.major_id
	AND par.parameter_id = ext.minor_id</sql>
