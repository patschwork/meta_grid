    <connection>Meta#Grid_Generic_Database</connection>
    <sql>WITH cte_CNT_db_table_changes AS
(
	SELECT 
		 id
		,COUNT(id) AS CNT_id 
	FROM (
		SELECT DISTINCT id,name,location FROM db_table_log
		WHERE fk_project_id = ${project_id}
	) A
	GROUP BY
	 id
	-- HAVING COUNT(id)&gt;1
)
,cte_Get_Last_Record AS
(
	SELECT id,MAX(log_id) AS MAX_log_id FROM db_table_log
	WHERE fk_project_id = ${project_id}
	GROUP BY id
)
,cte_Detect_Last_Deleted AS
(
	SELECT * FROM db_table_log DBTL
	INNER JOIN cte_Get_Last_Record cGLR ON
		cGLR.id=DBTL.id
	WHERE 
		    DBTL.log_action=&apos;DELETE&apos;
		AND fk_project_id = ${project_id}
)
SELECT 
	DISTINCT 
		 DBTL.id
		,DBTL.fk_object_type_id
		,DBTL.fk_project_id
		,DBTL.name
		,DBTL.location
		,cDLR.log_action
FROM db_table_log DBTL
INNER JOIN cte_CNT_db_table_changes CNT ON
	CNT.id=DBTL.id
LEFT JOIN cte_Detect_Last_Deleted cDLR ON
	cDLR.id=DBTL.id
WHERE 
	    cDLR.log_action IS NULL
	AND DBTL.location IS NOT NULL
	AND DBTL.fk_project_id = ${project_id}
ORDER BY 
	DBTL.id
</sql>
