    <data>
      <line>
        <item>SELECT 
    CAST(isr.ROUTINE_CATALOG AS VARCHAR(1500)) AS "ROUTINE_CATALOG"
   ,CAST(ns.nspname AS VARCHAR(1500)) AS "ROUTINE_SCHEMA"
   ,CAST(pr.proname AS VARCHAR(1500)) AS "ROUTINE_NAME"
   ,CAST(CASE
    	WHEN DATA_TYPE='void' THEN 'PROCEDURE'
	    ELSE ROUTINE_TYPE
	END AS VARCHAR(1500)) AS "ROUTINE_TYPE"
   ,CAST(pr.prosrc AS VARCHAR(4000)) AS "ROUTINE_DEFINITION"
   ,CAST(isr.ROUTINE_BODY AS VARCHAR(1500)) AS "ROUTINE_BODY"
   ,CAST(isr.DATA_TYPE AS VARCHAR(1500)) AS "DATA_TYPE"
   ,CAST(isr.SQL_DATA_ACCESS AS VARCHAR(1500)) AS "SQL_DATA_ACCESS"
   ,CAST(pg_catalog.obj_description(pr.oid, 'pg_proc') AS VARCHAR(4000)) as "Description"
   ,CAST(pr.oid AS INT) AS "RDBMS_internal_object_id"
FROM pg_proc pr,
     pg_type tp,
     pg_namespace ns,
     INFORMATION_SCHEMA.ROUTINES isr
 WHERE tp.oid = pr.prorettype
   AND pr.proisagg = FALSE
   AND tp.typname &lt;&gt; 'trigger'
   AND pr.pronamespace = ns.oid
   AND isr.specific_name = pr.proname||'_'||pr.oid
   AND pr.pronamespace IN (
       SELECT oid
         FROM pg_namespace
        -- WHERE nspname NOT LIKE 'pg_%'
        --  AND nspname != 'information_schema'
	)
ORDER BY
  1,2,3</item>
        <item>PGSQL</item>
      </line>
      <line>
        <item>SELECT 
    CAST(ROUTINE_CATALOG AS VARCHAR(1500)) AS [ROUTINE_CATALOG],
    CAST(ROUTINE_SCHEMA AS VARCHAR(1500)) AS [ROUTINE_SCHEMA],
    CAST(ROUTINE_NAME AS VARCHAR(1500)) AS [ROUTINE_NAME],
    CAST(ROUTINE_TYPE AS VARCHAR(1500)) AS [ROUTINE_TYPE],
    CAST(ROUTINE_DEFINITION AS VARCHAR(4000)) AS [ROUTINE_DEFINITION],
    CAST(ROUTINE_BODY AS VARCHAR(1500)) AS [ROUTINE_BODY],
    CAST(DATA_TYPE AS VARCHAR(1500)) AS [DATA_TYPE],
    CAST(SQL_DATA_ACCESS AS VARCHAR(1500)) AS [SQL_DATA_ACCESS],
    CAST(ext.[value] AS VARCHAR(4000)) AS [Description],
    CAST(obj.object_id AS INT) AS [RDBMS_internal_object_id]
FROM sys.objects obj
LEFT JOIN sys.extended_properties ext ON obj.object_id = ext.major_id AND ext.name = 'MS_Description'
LEFT JOIN sys.columns col ON col.object_id = ext.major_id
	AND col.column_id = ext.minor_id
LEFT JOIN sys.parameters par ON par.object_id = ext.major_id
	AND par.parameter_id = ext.minor_id
LEFT JOIN INFORMATION_SCHEMA.ROUTINES isr ON
    1 = 1
    AND isr.ROUTINE_SCHEMA = SCHEMA_NAME(obj.schema_id)
    AND isr.ROUTINE_NAME   = obj.NAME
WHERE 1 = 1
    AND obj.type_desc = 'SQL_STORED_PROCEDURE'
    AND par.object_id IS NULL -- no parameter, for the moment...
ORDER BY
  1,2,3</item>
        <item>MSSQL</item>
      </line>
