	<changeSet author="patrick_schmitz" id="000076" context="scheduling">

        <!-- Those views are automatically generated with a python helper tool: dwh_meta_v2\helper_tools\Python\database\create_wwwsqldesigner_model_additional_search_views.py -->
        <createView replaceIfExists="true" viewName="v_scheduling_SearchInterface">
        <![CDATA[
        SELECT
                id
                ,IFNULL(uuid, '') AS uuid
                ,fk_object_type_id
                ,fk_project_id
                ,IFNULL(name, '') AS name
                ,IFNULL(description, '') AS description
                ,fk_tool_id
                ,IFNULL(scheduling_series, '') AS scheduling_series
                ,fk_deleted_status_id
        FROM scheduling
        ]]>
        </createView>

        <comment>
        <![CDATA[
        Used objects:
                v_scheduling_SearchInterface
        Phabricator tasks:
                T64
        ]]>
        </comment>

	</changeSet>
</databaseChangeLog>
